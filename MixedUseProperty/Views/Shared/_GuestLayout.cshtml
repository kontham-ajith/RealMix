<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mixed Use Property Management</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="padding-top:5px;">

    <div class="col-xs-6" style="height:50%;vertical-align:top;top:0px;">

        <img src="~/Images/Propimg1.JPG" />
    </div>


    <div class="container body-content col-xs-6" style="text-align:center;width:40%;">
        <h1 style="text-align:center;color:orangered;">
            Real Mix
        </h1>
        <h3 style="text-align:center;color:orangered;">
            Mixed Use Property Management
        </h3>
        <span style="font-size:medium;text-align:right;">Ready to experience new level Mixed use Property Management?</span>
        <div class="col-xs-12" style="text-align:center;">
            <content>
                @RenderBody()
            </content>

        </div>


    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
